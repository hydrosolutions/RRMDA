    </exchangeItems>
    
    <doCleanUp>false</doCleanUp>
    
    <!-- specify where to store the restart files of the stoch model instances and which files to store. -->
    <restartInfo dirPrefix="ModelRestart_">
        <modelStateFile>E.mat</modelStateFile>
    </restartInfo>
    
</blackBoxModelConfig>
